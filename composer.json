{
    "name": "vdlp/oc-sitemapgenerators-plugin",
    "description": "Provides Sitemap Generators for the Vdlp.Sitemap plugin",
    "license": "GPL-2.0-only",
    "type": "october-plugin",
    "authors": [
        {
            "name": "Van der Let & Partners",
            "email": "octobercms@vdlp.nl"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "composer/installers": "^1.0 || ^2.0",
        "winter/storm": "^1.2",
        "vdlp/oc-sitemap-plugin": "dev-winter-cms-support"
    },
    "suggest": {
        "rainlab/pages-plugin": "This plugin has support for the RainLab.Pages plugin."
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://gateway.octobercms.com"
        },
		{
			"type": "vcs",
			"url": "git@github.com:michaelheider/oc-sitemap-plugin.git"
		}
    ],
    "minimum-stability": "stable",
    "archive": {
        "exclude": [
            ".gitattributes",
            ".github",
            ".gitignore"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
